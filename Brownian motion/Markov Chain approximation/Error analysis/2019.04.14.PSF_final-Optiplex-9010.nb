Cell["\<\
Hence we are left with an exponentially decaying term and the EMSF error\
\>", "Text",
 CellChangeTimes->{{3.7642313126515803`*^9, 3.7642313238684263`*^9}}],

Cell[BoxData[
 RowBox[{
  FractionBox["1", "2"], " ", 
  SuperscriptBox["\[ExponentialE]", 
   RowBox[{
    RowBox[{
     RowBox[{"-", "2"}], " ", "g1", " ", "n", " ", 
     RowBox[{"(", 
      RowBox[{"g2", "+", "x"}], ")"}]}], "-", 
    FractionBox[
     RowBox[{"2", " ", 
      SuperscriptBox[
       RowBox[{"(", 
        RowBox[{"\[Pi]", " ", "s"}], ")"}], "2"]}], "n"], " ", "+", " ", 
    RowBox[{"\[ImaginaryI]", " ", "n", " ", 
     RowBox[{"(", 
      RowBox[{"g2", "+", "x", "-", "\[Theta]"}], ")"}]}]}]], 
  RowBox[{"(", " ", 
   RowBox[{
    RowBox[{
     SuperscriptBox["\[ExponentialE]", 
      RowBox[{
       RowBox[{"4", " ", "\[ImaginaryI]", " ", "\[Pi]", " ", "s", " ", "x"}], 
       "+", 
       RowBox[{"2", " ", "g1", " ", "n", " ", 
        RowBox[{"(", 
         RowBox[{"g2", "+", "x"}], ")"}]}]}]], " ", 
     RowBox[{"(", 
      RowBox[{"2", "  ", "+", 
       FractionBox[
        RowBox[{"Exp", "[", 
         RowBox[{"-", 
          SuperscriptBox["z1", "2"]}], "]"}], 
        RowBox[{
