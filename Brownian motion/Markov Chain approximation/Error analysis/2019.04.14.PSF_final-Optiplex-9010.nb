Cell[TextData[{
 "Hence we replace ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Erfc", "[", 
    SubscriptBox["z", "1"]}], TraditionalForm]]],
 "] = ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Erfc", "[", 
    RowBox[{"-", 
     SubsuperscriptBox["z", "1", "*"]}]}], TraditionalForm]]],
 "] ~ 2 - ",
