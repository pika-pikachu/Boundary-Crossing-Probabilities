  FormBox[
   RowBox[{
    FractionBox[
     RowBox[{"Exp", "[", 
      RowBox[{"-", 
       SubsuperscriptBox["z", "1", 
        RowBox[{"*", "2"}]]}], "]"}], 
     RowBox[{
      SqrtBox["\[Pi]"], 
      SubsuperscriptBox["z", "1", "*"]}]], "=", 
    RowBox[{"2", " ", "+", " ", 
     FractionBox[
      RowBox[{"Exp", "[", 
       RowBox[{"-", 
        SubsuperscriptBox["z", "1", "2"]}], "]"}], 
      RowBox[{
       SqrtBox["\[Pi]"], 
       SubscriptBox["z", "1"]}]], " "}]}], TraditionalForm]]],
 "and -2 + ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Erfc", "[", 
    SubscriptBox["z", "2"]}], TraditionalForm]]],
 "] = -2 + ",
 Cell[BoxData[
  FormBox[
   RowBox[{"Erfc", "[", 
    RowBox[{"-", 
     SuperscriptBox[
      SubscriptBox["z", "2"], "*"]}]}], TraditionalForm]]],
 "] ~ ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"Exp", "[", 
     RowBox[{"-", 
      SubsuperscriptBox["z", "2", 
