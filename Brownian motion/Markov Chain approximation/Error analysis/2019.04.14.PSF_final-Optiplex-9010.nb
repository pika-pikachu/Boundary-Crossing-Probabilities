Cell[TextData[{
 "The multiplier is no longer so simple, and has dependence on x. Simplifying \
gives Exp( ",
 Cell[BoxData[
  FormBox[
   RowBox[{"2", "n", " ", 
    RowBox[{
     SubscriptBox["g", "1"], "(", 
     RowBox[{
      SubscriptBox["g", "1"], "-", 
      SubscriptBox["g", "2"]}], ")"}], " "}], TraditionalForm]]],
 " ",
 Cell[BoxData[
  FormBox[
   RowBox[{")", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "2"}], 
      SuperscriptBox["\[Pi]", "2"], 
      RowBox[{
       SuperscriptBox["s", "2"], "/", "n"}]}]}]}], TraditionalForm]]],
 "]Exp[2nx(g2  - g1)]"
}], "Text",
 CellChangeTimes->{{3.7642353385874624`*^9, 3.764235353292925*^9}, {
  3.764235485554632*^9, 3.7642356261155252`*^9}}],

